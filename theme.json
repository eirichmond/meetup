{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ],
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "X-Small",
					"size": "max(0.6rem, 1vw)",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "max(0.8rem, 3vw)",
					"slug": "40"
				},
				{
					"name": "Medium",
					"size": "max(1rem, 5vw)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "max(2rem, 8vw)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "max(2.5rem, 10vw)",
					"slug": "70"
				}
			]
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"name": "Black",
					"color": "#000000",
					"slug": "black"
				},
				{
					"name": "White",
					"color": "#ffffff",
					"slug": "white"
				},
				{
					"name": "Primary",
					"color": "#4f5d75",
					"slug": "primary"
				},
				{
					"name": "Secondary",
					"color": "#d7263d",
					"slug": "secondary"
				},
				{
					"name": "Tertiary",
					"color": "#087e8b",
					"slug": "tertiary"
				},
				{
					"name": "Quaternary",
					"color": "#2d3142",
					"slug": "quaternary"
				},
				{
					"name": "Quinary",
					"color": "#bfc0c0",
					"slug": "quinary"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"fluid":true,
					"name": "Small",
					"size": "clamp(0.8333rem, 0.7406rem + 0.4638vw, 1.1rem)",
					"slug": "small"
				},
				{
					"fluid":true,
					"name": "Medium",
					"size": "clamp(1rem, 0.8696rem + 0.6522vw, 1.375rem)",
					"slug": "small"
				},
				{
					"fluid":true,
					"name": "Large",
					"size": "clamp(1.2rem, 1.0196rem + 0.9022vw, 1.7188rem)",
					"slug": "large"
				},
				{
					"fluid":true,
					"name": "X-Large",
					"size": "clamp(1.44rem, 1.1936rem + 1.2321vw, 2.1484rem)",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"blockGap":"0px"
		},
		"typography": {
			"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}
